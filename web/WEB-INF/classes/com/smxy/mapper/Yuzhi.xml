<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
    <mapper namespace="com.smxy.mapper.YuzhiMapper">
        <update id="updateMq2" parameterType="Yuzhi" >
            update yuzhi_ set mq2=#{mq2} where id=1
        </update>
    <update id="updateTemp" parameterType="Yuzhi" >
            update yuzhi_ set temp=#{temp} where id=1
        </update>
        <select id="listYuzhi" resultType="Yuzhi">
            select * from  yuzhi_
        </select>
    </mapper>